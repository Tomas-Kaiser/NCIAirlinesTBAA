<p id="alert"><%= alert %></p>

<div class="registration-form">
  <header>
    <h1>Login</h1>
  </header>
  <%= form_tag sessions_path do |form| %>
    <div class="field">
      <%= label_tag :email %>
      <%= text_field_tag :email %>
    </div>
    <div class="field">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <div class="actions">
      <%= submit_tag "Login" %>
    </div>
  <% end %>  
</div>

